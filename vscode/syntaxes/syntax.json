{
  "scopeName": "source.atlang",
  "patterns": [
    {
      "match": "@[a-zA-Z_][a-zA-Z0-9_]*",
      "name": "variable.atlang"
    },
    {
      "match": "(\"\"[^\"]*\"\")",
      "name": "string.atlang"
    },
    {
      "match": "\\b(if|else)\\b",
      "name": "keyword.control.atlang"
    },
    {
      "match": "\\b(print|getEnv|startServer|getWeb|postWeb)\\b",
      "name": "support.function.atlang"
    },
    {
      "match": "[{}]",
      "name": "punctuation.brace.atlang"
    },
    {
      "match": "=",
      "name": "keyword.operator.assignment.atlang"
    },
    {
      "match": "\\+",
      "name": "keyword.operator.arithmetic.atlang"
    }
  ],
  "repository": {}
}
